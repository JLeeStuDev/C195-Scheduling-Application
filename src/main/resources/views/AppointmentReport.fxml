<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox style="-fx-padding: 10 10 10 10" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AppointmentReportController">
    <Label fx:id="appointmentReportLabel" />
    <TextArea fx:id="reportText" editable="false" prefHeight="350.0" prefWidth="580.0" wrapText="true">
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding></TextArea>
    <Button onAction="#closeWindow" text="Close">
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin></Button>
</VBox>
